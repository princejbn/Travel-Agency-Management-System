    <section class="ftco-section contact-section">
      <div class="container">
        <div class="row block-9 mb-4">
          <div class="col-md-6 pr-md-5 flex-column">
            <div class="row d-block flex-row">
              <h2 class="h4 mb-4">Contact Information</h2>
              <div class="col mb-3 d-flex py-4 border" style="background: white;">
                <div class="align-self-center">
                  <p class="mb-0"><span>Address:</span>SJB Institute Of Technology</p>
                </div>
              </div>
              <div class="col mb-3 d-flex py-4 border" style="background: white;">
                <div class="align-self-center">
                  <p class="mb-0"><span>Phone:</span> <a href="tel://1234567920">+ 1235 2355 98</a></p>
                </div>
              </div>
              <div class="col mb-3 d-flex py-4 border" style="background: white;">
                <div class="align-self-center">
                  <p class="mb-0"><span>Email:</span> <a href="mailto:info@yoursite.com">info@bookmytrip.com</a></p>
                </div>
              </div>
              <div class="col mb-3 d-flex py-4 border" style="background: white;">
                <div class="align-self-center">
                  <p class="mb-0"><span>Website</span> <a href="#">bookmytrip.com</a></p>
                </div>
              </div>
            </div>
          </div>
          <div class="col-md-6">
            <form method="POST" action="/contact?q=feedback" >
              <% if(typeof(msg)!='undefined'){%>
                <p style="color: red"><%=msg%></p>
              <%}%>
              <div class="form-group">
                <input type="text" class="form-control" name="name" id="name" placeholder="Your Name" maxlength="30">
              </div>
              <div class="form-group">
                <input type="text" class="form-control" name="email" id="email" placeholder="Your Email" maxlength="50">
              </div>
              <div class="form-group">
                <input type="text" class="form-control" name="subject" id="subject" placeholder="Subject" maxlength="100">
              </div>
              <div class="form-group">
                <textarea name="message" id="message" cols="30" rows="7" class="form-control" placeholder="Message" maxlength="190"></textarea>
              </div>
              <div class="form-group">
                <input type="submit" value="Send Feedback" class="btn btn-primary py-3 px-5">
              </div>
            </form>
          </div>
        </div>
        
       <!--  <div class="row mt-5">
